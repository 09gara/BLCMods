        #<Rocket Ammo and Grenades in Early Game>

            # Lets rocket ammo and grenades be available in world drops and vending
            # machines right from the start of the game.  (Grenades already pop up
            # pretty early, actually, but this should get 'em right from level 1.)

            #<hotfix><key>"SparkLevelPatchEntry-ApocRocketVending0"</key><value>",GD_ItemGrades.Ammo_Shop.ItemGrade_AmmoShop_RocketLauncher,Manufacturers[0].Grades[0].GameStageRequirement.MinGameStage,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocRocketDrops0"</key><value>",GD_ItemGrades.Ammo.ItemGrade_Ammo_RocketLauncher,Manufacturers[0].Grades[0].GameStageRequirement.MinGameStage,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocGrenadeVending0"</key><value>",GD_ItemGrades.Ammo_Shop.ItemGrade_AmmoShop_Grenade,Manufacturers[0].Grades[0].GameStageRequirement.MinGameStage,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocGrenadeDrops0"</key><value>",GD_ItemGrades.Ammo.ItemGrade_Ammo_Grenade,Manufacturers[0].Grades[0].GameStageRequirement.MinGameStage,,1"</value><on>

        #</Rocket Ammo and Grenades in Early Game>

        #<Exhaustive Weapon/Item Part Unlocks>

            # This is an utterly absurd set of commands which sets every weapon/item part
            # to be able to spawn from the very beginning.  This is what will allow all
            # element types from the beginning game, and various COM parts, etc.
            # Realistically we'd only need a subset of these, but we include all fixes
            # for unique/legendary partlists as well.  These are all generated by some
            # code which reads from FilterTool's catalogued `obj dump` outputs.  Rather
            # than alter MinGameStageIndex itself, as we do on my BL2 Better Loot mod,
            # here we're editing the ConsolidatedAttributeInitData vars which drive those.

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock0"</key><value>",gd_cork_weap_assaultrifle.A_Weapons_Unique.AR_Vladof_3_Hail:PartList,ConsolidatedAttributeInitData[4].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock1"</key><value>",gd_cork_weap_assaultrifle.A_Weapons_Unique.AR_Vladof_3_Hail:PartList,ConsolidatedAttributeInitData[5].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock2"</key><value>",gd_cork_weap_assaultrifle.A_Weapons_Unique.AR_Vladof_3_Hail:PartList,ConsolidatedAttributeInitData[6].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock3"</key><value>",gd_cork_weap_assaultrifle.A_Weapons_Unique.AR_Vladof_3_Hail:PartList,ConsolidatedAttributeInitData[7].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock4"</key><value>",GD_Cork_Weap_Lasers.A_Weapons.Laser_Dahl:PartList,ConsolidatedAttributeInitData[2].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock5"</key><value>",GD_Cork_Weap_Lasers.A_Weapons.Laser_Dahl:PartList,ConsolidatedAttributeInitData[3].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock6"</key><value>",GD_Cork_Weap_Lasers.A_Weapons.Laser_Dahl:PartList,ConsolidatedAttributeInitData[4].BaseValueConstant,,1"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocPartUnlock7"</key><value>",GD_Cork_Weap_Lasers.A_Weapons.Laser_Dahl_2_Uncommon:PartList,ConsolidatedAttributeInitData[5].BaseValueConstant,,1"</value><on>

