            #<hotfix><key>"SparkLevelPatchEntry-ApocLockerLoot1"</key><value>",GD_Itempools.ListDefs.StorageLockerLoot,LootData[5].ItemAttachments[1].ItemPool,,ItemPoolDefinition'GD_Itempools.GrenadeModPools.Pool_GrenadeMods_04_Rare'"</value><on>

        #</Better Lockers>

        #<Better Safes>

            # Improves Safes - the amount of money is increased, Moonstone results are
            # far more common, and if there is gear drops, they are guaranteed
