    #<Loot Pool Tweaks>

        #<Normalize Gun Type Drops>

            # In the vanilla game, pistols are the most likely drop, followed by
            # ARs/SMGs/Shotguns, then snipers/lasers, and then launchers.  This
            # normalizes the probabilities for Pistols/ARs/SMGs/Shotguns, and
            # makes snipers, lasers, and launchers more likely (though still not
            # as likely as the others)

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapCommonPistol0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_01_Common,BalancedItems[0].Probability.BaseValueConstant,,100"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapUncommonPistol0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_02_Uncommon,BalancedItems[0].Probability.BaseValueConstant,,100"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapRarePistol0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_04_Rare,BalancedItems[0].Probability.BaseValueConstant,,100"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapVeryRarePistol0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_05_VeryRare,BalancedItems[0].Probability.BaseValueConstant,,100"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapLegendaryPistol0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_06_Legendary,BalancedItems[0].Probability.BaseValueConstant,,100"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapCommonAR0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_01_Common,BalancedItems[1].Probability.BaseValueConstant,,100"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocNormWeapUncommonAR0"</key><value>",GD_Itempools.WeaponPools.Pool_Weapons_All_02_Uncommon,BalancedItems[1].Probability.BaseValueConstant,,100"</value><on>

