#<No Phase-Loss>

    #<Description>

        Made by Glory.

        This mod removes the reduction in Phaselock duration when it's used on the same foe multiple times.

    #</Description>

    #<Code>

        #<hotfix><key>"SparkOnDemandPatchEntry-No Phase-Loss1"</key><value>"GD_Siren_Streaming,GD_Siren_Skills.Phaselock.Skill_Phaselock_DiminishingReturns,SkillEffectDefinitions,,((AttributeToModify=AttributeDefinition'GD_Siren_Skills.Attributes.PhaselockTimeScale',bIncludeDuelingTargets=False,bIncludeSelfAsTarget=False,bOnlyEffectTargetsInRange=False,bExcludeNonPlayerCharacters=False,EffectTarget=TARGET_Self,TargetInstanceDataName=,TargetCriteria=CRITERIA_None,ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=0.000000),GradeToStartApplyingEffect=0,PerGradeUpgradeInterval=1,PerGradeUpgrade=(BaseValueConstant=0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=0.000000),BonusUpgradeList=))"</value><on>

    #</Code>

#</No Phase-Loss>

set Transient.SparkServiceConfiguration_6 Keys ("SparkOnDemandPatchEntry-No Phase-Loss1")

set Transient.SparkServiceConfiguration_6 Values ("GD_Siren_Streaming,GD_Siren_Skills.Phaselock.Skill_Phaselock_DiminishingReturns,SkillEffectDefinitions,,((AttributeToModify=AttributeDefinition'GD_Siren_Skills.Attributes.PhaselockTimeScale',bIncludeDuelingTargets=False,bIncludeSelfAsTarget=False,bOnlyEffectTargetsInRange=False,bExcludeNonPlayerCharacters=False,EffectTarget=TARGET_Self,TargetInstanceDataName=,TargetCriteria=CRITERIA_None,ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=0.000000),GradeToStartApplyingEffect=0,PerGradeUpgradeInterval=1,PerGradeUpgrade=(BaseValueConstant=0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=0.000000),BonusUpgradeList=))")

