===============================================
flamethrower, a REEL flamethrower.

replace the swabar

braek the Logans Gun Projectiles

make you slower

+damage

-fire rate

+mag size

hf

created by ozay(or oz yohay)
===============================================

##name##



set GD_Gladiolus_Weapons.Name.Title.Title_Pearlescent_Sawbar PartName flamethrower

set GD_Gladiolus_Weapons.Name.Title.Title_Pearlescent_Sawbar:AttributePresentationDefinition_8 NoConstraintText fire, fire every where.

##-speed boost##

set GD_Gladiolus_Weapons.AssaultRifle.AR_Barrel_Jakobs_Sawbar ExternalAttributeEffects ((AttributeToModify=AttributeDefinition'D_Attributes.GameplayAttributes.FootSpeed',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-0.500000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),

##stats##




set GD_Gladiolus_Weapons.AssaultRifle.AR_Barrel_Jakobs_Sawbar WeaponAttributeEffects ((AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponDamage',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=2.200000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponClipSize',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=100.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.Weapo sPerShot',ModifierType=MT_PreAdd,BaseModifierValue=(BaseValueConstant=0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponFireInterval',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-10.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponSpread',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.GameplayAttributes.PlayerCriticalHitBonus',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponShotCost',ModifierType=MT_PostAdd,BaseModifierValue=(BaseValueConstant=-2.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponPerShotAccuracyImpulse',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-90.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)))

##parts##



set GD_Gladiolus_Weapons.AssaultRifle.AR_Barrel_Jakobs_Sawbar GestaltModeSkeletalMeshName AR_Barrel_Alien


##Shots##



set GD_Gladiolus_Weapons.AssaultRifle.AR_Barrel_Jakobs_Sawbar CustomFiringModeDefinition FiringModeDefinition 'GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun'

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun FireType EWWFT_Bullet

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun PartSysTemplate ParticleSystem 'FX_WEP_Grenades.Particles.Part_AOEGrenade_FlameSpurt'

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun Lifetime 5.000000

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun OnAnyImpact ()

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun bSuppressImpactEffects True

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun FiringPatternLines ()

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun Speed 1000.000000

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun NumRicochets 0

set GD_Weap_Pistol.FiringModes.Bullet_Pistol_LogansGun bPenetratePawn True

##skin##

set GD_Gladiolus_Weapons.ManufacturerMaterials.Mat_Bandit_6_Sawbar Material MaterialInstanceConstant 'Orchid_GunMaterials.Materials.AssaultRifle.Mati_DahlCommonAR_Seraphim'

