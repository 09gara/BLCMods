##skin##

set Common_GunMaterials.Materials.Launcher.Mati_BanditRareRL Parent Common_GunMaterials.Materials.Launcher.Mati_MaliwanEpicRL

##name##



set GD_Weap_Launchers.Name.Title_Torgue.Title_Launcher PartName Zaeden Blaster

set GD_Weap_Launchers.Name.Title_Torgue.Title_Launcher:AttributePresentationDefinition_8 NoConstraintText (Not OP)
#Rarity

set GD_Sage_HarpoonGun.Barrel.Sage_HarpoonGun_Barrel rarity (basevalueattribute=none)

set GD_Sage_HarpoonGun.Barrel.Sage_HarpoonGun_Barrel rarity (BaseValueConstant=7)

##stats##




set GD_Sage_HarpoonGun.Barrel.Sage_HarpoonGun_Barrel WeaponAttributeEffects ((AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponDamage',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-14.500000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponClipSize',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=2.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponProjectilesPerShot',ModifierType=MT_PreAdd,BaseModifierValue=(BaseValueConstant=9.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponFireInterval',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-2.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponSpread',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-2.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponReloadSpeed',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=0.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponShotCost',ModifierType=MT_PostAdd,BaseModifierValue=(BaseValueConstant=-3.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponPerShotAccuracyImpulse',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=-9999.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)))



##Shots##

set GD_Sage_HarpoonGun.Barrel.Sage_HarpoonGun_Barrel CustomFiringModeDefinition FiringModeDefinition 'GD_Weap_SMG.FiringModes.Bullet_Plasma_Default'

##parts##


set GD_Sage_HarpoonGun.Barrel.Sage_HarpoonGun_Barrel GestaltModeSkeletalMeshName L_Bsafasdf


