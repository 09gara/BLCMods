#<BL2 Movement Speed Cheats>

    # BL2 Movement Speed Cheats v1.0.0
    # Licensed under Public Domain / CC0 1.0 Universal
    #
    # Ups the movement speed of all characters (including while crouched and during
    # FFYL), increases jump height a bit, and provides much more air control.

    #<Axton>

        # The set statements apply the settings initially

        set GD_Soldier.Character.CharClass_Soldier GroundSpeed 1000

        set GD_Soldier.Character.CharClass_Soldier AirSpeed 1100

        set GD_Soldier.Character.CharClass_Soldier CrouchedPct 1

        set GD_Soldier.Character.CharClass_Soldier JumpZ 900

        # The hotfixes apply once you respawn or exit FFYL
        # (These may be a bit overboard.  Not sure if you need the BaseValue
        # ones, or if the BaseValue ones are the only ones you need, etc.)

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonGroundSpeed0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,GroundSpeed,,1000"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonGroundSpeedBaseValue0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,GroundSpeedBaseValue,,1000"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonAirSpeed0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,AirSpeed,,1100"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonAirSpeedBaseValue0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,AirSpeedBaseValue,,1100"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonJumpZ0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,JumpZ,,900"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonJumpZBaseValue0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,JumpZBaseValue,,900"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAxtonCrouchedPct0"</key><value>"GD_Soldier_Streaming,GD_Soldier_Streaming.Pawn_Soldier,CrouchedPct,,1"</value><on>

    #</Axton>

    #<Gaige>

        # The set statements apply the settings initially

        set GD_Tulip_Mechromancer.Character.CharClass_Mechromancer GroundSpeed 1000

