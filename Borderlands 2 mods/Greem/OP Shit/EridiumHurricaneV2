		#<Norfleet Shots>
		
			set GD_Sage_RaidWeapons.AssaultRifle.AR_Barrel_LeadStorm CustomFiringModeDefinition FiringModeDefinition 'GD_Weap_Launchers.FiringModes.FM_Alien_Maliwan_Norfleet'

