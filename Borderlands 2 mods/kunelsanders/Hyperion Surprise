#####################
Hyperion Surprise
Small change I made for funsies
Locks the Gwen's Head to full Hyperion and gives it 100% accuracy
#####################

set GD_Weap_Pistol.Name.Title.Title__Unique_GwensHead PartName Hyperion Surprise
set GD_Weap_Pistol.Name.Title.Title__Unique_GwensHead:AttributePresentationDefinition_8 NoConstraintText Ta-daa!

set GD_Weap_Pistol.Barrel.Pistol_Barrel_Hyperion_GwensHead AttributeSlotUpgrades ((SlotName="WeaponSpread",GradeIncrease=9999,bActivateSlot=True),(SlotName="AccuracyMin",GradeIncrease=10,bActivateSlot=True),(SlotName="AccuracyMax",GradeIncrease=20,bActivateSlot=True),(SlotName="AccuracyRegen",GradeIncrease=10,bActivateSlot=True))) 

set GD_Weap_Pistol.A_Weapons_Unique.Pistol_Dahl_3_GwensHead:WeaponPartListCollectionDefinition_109 GripPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Grip.Pistol_Grip_Hyperion',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=2)))
set GD_Weap_Pistol.A_Weapons_Unique.Pistol_Dahl_3_GwensHead:WeaponPartListCollectionDefinition_109 SightPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Sight.Pistol_Sight_Hyperion',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=2)))
